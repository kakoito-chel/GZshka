for x in range(50):
    for y in range(50):
        for z in range(50):
            s = '0'+'1'* x +'2'* y +'3'* z + '0'
            s1=s
            while "00" not in s1:
                s1 = s1.replace('01', '210', 1)
                s1 = s1.replace('02', '3101', 1)
                s1 = s1.replace('03', '2012', 1)
            if s1.count('1') == 56 and s1.count('2') == 44 and s1.count('3') == 19:
                print(len(s))
